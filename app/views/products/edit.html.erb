<div class="product-box">
  <h1> Edit Pet </h1>

  <%= bootstrap_form_for @product do |f| %>

  <div>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.text_field :price %>
  </div>

  <div>
    <%= f.text_area :description %>
  </div>


  <div>
    <%= f.submit 'Save Pet' %>
  </div>

  <% end %>
</div>